<%= render "users/show" %>
<div class="myteam">
    <%= image_tag @team.avatar.url %>
    <div class="team_name">
        <%= @team.name %>
    </div>
    <div class="members">
        <ul>
            <% @users.each do |user| %>
                <li>
                    <%= link_to user.name, user_path(user) %>
                </li>
            <% end %>
        <ul>
    </div>
    <%= link_to "編集", "#"%>
</div>