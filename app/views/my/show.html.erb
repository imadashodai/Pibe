<div class="sidebar">
    <%= render "my/show" %>
    <%= render "my/sidevar" %>
</div>
<div class="members">
    <h1>Members</h1>
    <ul>
        <% @team_users.each do |user| %>
            <li>
                <%= image_tag user.avatar.url, :alt => "noimage" %><br>
                <%= link_to user.name, user_path(user) %>
            </li>
        <% end %>
    <ul>
</div>